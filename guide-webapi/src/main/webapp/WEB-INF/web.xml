<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Test 'An' and 'Weapon' code Web Application</display-name>

	<servlet>
		<servlet-name>pp</servlet-name>
		<servlet-class>org.guide.webapi.servlet.print.PrintParamsServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>member</servlet-name>
		<servlet-class>org.guide.webapi.servlet.MemberServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ehcache</servlet-name>
		<servlet-class>org.guide.webapi.servlet.EhCacheServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>memcached</servlet-name>
		<servlet-class>org.guide.webapi.servlet.MemcachedServlet</servlet-class>
	</servlet>

	<!-- mapping -->

	<servlet-mapping>
		<servlet-name>pp</servlet-name>
		<url-pattern>/pp/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>member</servlet-name>
		<url-pattern>/member</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ehcache</servlet-name>
		<url-pattern>/ehcache</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>memcached</servlet-name>
		<url-pattern>/memcached</url-pattern>
	</servlet-mapping>
</web-app>
